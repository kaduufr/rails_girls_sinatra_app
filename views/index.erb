
	<h1><%= @title%></h1>
	<p>O que ha para o jantar? <%= @tite %></p>
	<form action="cast" method="post">
		<ul class="unstyled">
			<% Choices.each do |id, text| %>
				<li>
					<label class="radio">
						<input type="radio" name="vote" value="<%= id%>" id="vote_<%= id%>">
						<%= text %>
					</label>
				</li>
			<%end %>
		</ul>
		<button class="btn btn-primary" type="submit">Cast this vote</button>
	</form>
